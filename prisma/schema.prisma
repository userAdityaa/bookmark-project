generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model User {
  id        Int        @id @default(autoincrement())
  name      String
  icon      String
  email     String     @unique
  password  String
  bookmarks Bookmark[]
}

model Bookmark {
  id        Int        @id @default(autoincrement())
  name      String
  icon      String
  createdAt DateTime   @default(now())
  userId    Int
  User      User       @relation(fields: [userId], references: [id])
  listItems ListItem[]
}

model ListItem {
  id         Int      @id @default(autoincrement())
  icon       String
  name       String
  link       String
  createdAt  DateTime @default(now())
  bookmarkId Int
  bookmark   Bookmark @relation(fields: [bookmarkId], references: [id])
}
