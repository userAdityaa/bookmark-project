generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bookmark {
  id        Int        @id @default(autoincrement())
  name      String
  icon      String
  listItems ListItem[]
  createdAt DateTime   @default(now())
}

model ListItem {
  id         Int      @id @default(autoincrement())
  icon       String
  name       String
  link       String
  createdAt  DateTime @default(now())
  bookmark   Bookmark @relation(fields: [bookmarkId], references: [id])
  bookmarkId Int
}
